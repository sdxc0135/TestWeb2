CREATE TABLE TESTWEB_USER 
(
  ID VARCHAR2(50 BYTE) NOT NULL 
, PW VARCHAR2(50 BYTE) NOT NULL 
, NAME VARCHAR2(50 BYTE) NOT NULL 
, PHONE_NUMBER1 VARCHAR2(20 BYTE) NOT NULL 
, PHONE_NUMBER2 VARCHAR2(20 BYTE) NOT NULL 
, PHONE_NUMBER3 VARCHAR2(20 BYTE) NOT NULL 
, EMAIL1 VARCHAR2(50 BYTE) 
, EMAIL2 VARCHAR2(50 BYTE) 
, ADDRESS1 VARCHAR2(50 BYTE) 
, ADDRESS2 VARCHAR2(50 BYTE) 
, REGDATE DATE DEFAULT SYSDATE 
, CONSTRAINT TESTWEB_USER_PK PRIMARY KEY(ID)
  
) 

CREATE VIEW USER_VIEW AS (SELECT ID, PW FROM TESTWEB_USER);
